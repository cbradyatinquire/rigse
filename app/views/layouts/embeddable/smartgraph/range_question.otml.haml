!!! XML
= render :partial => "otml/character_entities"
%otrunk{ :id => @smartgraph_range_question.uuid}
  =ot_imports
  %objects
    %OTSystem
      =ot_bundles(:title => @smartgraph_range_question.name)
      %root
        %OTCompoundDoc
          %bodyText
            #content
              %h3
                = Embeddable::Smartgraph::RangeQuestion.display_name + ': ' + @smartgraph_range_question.name
              %object{ :refid => ot_refid_for(@smartgraph_range_question) }
      %library
        = content_for :layout