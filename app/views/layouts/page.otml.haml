!!! XML
= render :partial => "otml/character_entities"
%otrunk{ :id => @page.uuid }
  =ot_imports
  %objects
    %OTSystem{:local_id => 'system'}
      =ot_bundles({:title => h(@page.name), :use_scroll_pane => true})
      %root
        = content_for dom_id_for(@page, :page)
      %library
        %OTLabbook{:local_id =>'labbook'}/
        = content_for :library